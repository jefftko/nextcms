    "@tailwindcss/forms": "^0.5.4",
    "@tailwindcss/typography": "^0.5.9",
    "aos": "^2.3.4",
    "autoprefixer": "^10.4.13",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "contentlayer2": "^0.4.6",
    "dayjs": "^1.11.11",
    "easymde": "^2.18.0",
    "esbuild": "0.18.11",
    "flatpickr": "^4.6.13",
    "formidable": "^3.5.1",
    "github-slugger": "^1.4.0",
    "gray-matter": "^4.0.2",
    "image-size": "1.0.0",
